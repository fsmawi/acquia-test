{
  "name": "pipelines-ux",
  "version": "0.1.3",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "echo PLEASE START MERVER at 3000 ie. npm run mock PATH_TO_YAML && ng serve --env=mock",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check",
    "eslint": "eslint test/**/*.js",
    "test": "ng test --watch=false",
    "build:server": "cp -r server/ dist/server && cp -r environments/vendor dist/server/vendor",
    "mock": "merver",
    "mock:websocket": "node scripts/mock-websocket.js",
    "mock:jobs": "merver test/api-mock-resources/jobs.yml",
    "mock:no-jobs": "merver test/api-mock-resources/no-jobs.yml",
    "coverage": "ng test --code-coverage --watch=false",
    "e2e:ode": "PIPELINES_URL=`cat ~/ode.url`\\/index.html# CONFIG=\"test/e2e/aqtestfile.js\" SERVICE_USERNAME=$SAUCE_SERVICE_USERNAME SERVICE_KEY=$SAUCE_SERVICE_KEY aqtest --host sauce --test-name Pipelines_ODE_Test --service-platform MAC --browser chrome --tags ~@pending test/e2e/features",
    "e2e:parallel:local": "node scripts/parallel-e2e.js",
    "e2e:acceptance": "echo TODO",
    "integration": "mocha test/integration --recursive",
    "integration:acceptance": "mocha test/integration --recursive",
    "pipelines:build:master": "ng build --aot --env=live-mock --output-path=/tmp/build",
    "pipelines:build:staging": "ng build --aot --env=live-mock --output-path=/tmp/build",
    "pipelines:build:production": "ng build --prod --aot --progress --output-path=/tmp/build",
    "changelog": "standard-version --first-release"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/flex-layout": "^2.0.0-beta.4",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/material": "2.0.0-beta.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "angular2-moment": "^1.1.0",
    "clipboard": "^1.6.0",
    "ansi_up": "^1.3.0",
    "core-js": "^2.4.1",
    "moment": "^2.17.1",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "0.7.6",
    "fs-extra": "^2.0.0"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.3.1",
    "@types/clipboard": "^1.5.31",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.26",
    "aqtest": "file:lib/aqtest-2.1.2.tgz",
    "async": "^2.1.5",
    "chai": "^3.5.0",
    "codelyzer": "~2.0.0-beta.1",
    "colors": "^1.1.2",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.15.0",
    "eslint-config-google": "^0.7.1",
    "glob": "^7.1.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "karma-spec-reporter": "0.0.30",
    "merver": "git+https://github.com/raghunat/merver.git",
    "mocha": "^3.2.0",
    "protractor": "~4.0.13",
    "querystring": "^0.2.0",
    "supertest": "^3.0.0",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.3",
    "ws": "^2.2.0",
    "standard-version": "2.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
